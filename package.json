{
  "name": "bup",
  "version": "5.0.1",
  "description": "Automated rollup build based on babelrc",
  "license": "BSD-3-Clause",
  "author": "SÃ©rgio Ramos <mail@sergioramos.me>",
  "repository": "github:ramitos/bup",
  "keywords": [
    "babel",
    "rollup",
    "config",
    "babelrc",
    "lib"
  ],
  "files": [
    "bin"
  ],
  "bin": "bin/index.js",
  "scripts": {
    "fmt": "prettier --config .prettierrc --write '**/*'",
    "lint": "eslint . --ext .js"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "apr-for-each": "^3.0.3",
    "apr-main": "^4.0.3",
    "apr-parallel": "^3.0.3",
    "babelrc-rollup": "3.0.0",
    "camel-case": "^3.0.0",
    "chokidar": "^2.1.2",
    "lodash.debounce": "^4.0.8",
    "make-dir": "^2.1.0",
    "mz": "^2.7.0",
    "param-case": "^2.1.1",
    "pkg-dir": "^4.0.0",
    "read-pkg": "^5.0.0",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "eslint": "5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-xo-space": "^0.21.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "*.*": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  }
}
